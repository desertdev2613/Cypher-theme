{
	"name": "Cypher",
	"type": "dark",
	"colors": {
		"editor.background": "#0a0a0a",
		"editor.foreground": "#a6aaac",
		"activityBarBadge.background": "#48958f",
		"foreground": "#f0f3f2",
		"focusBorder": "#acd4c3",
		"input.background": "#2f5c5c",
		"selection.background": "#65aad1",
		"textLink.foreground": "#acd4c3",
		"textLink.activeForeground": "#cecece",
		"button.background": "#c7768e",
		"dropdown.background": "#21318a",
		"activityBar.background": "#0c0b0b",
