		"list.activeSelectionBackground": "#659c7f",
		"list.inactiveSelectionBackground": "#101113",
		"sideBar.background": "#101113",
		"editorGroupHeader.tabsBackground": "#101113",
		"editorWhitespace.foreground": "#21318a",
